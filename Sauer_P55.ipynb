   "source": [
    "# p55\n",
    "from sympy import *\n",
    "x, r, c, m = symbols('x r c m')\n",
    "g = symbols('g', cls = Function)\n",
    "f = g(x) * (x - r)**m\n",
    "print(f)\n",
    "e_i0 = x - r\n",
    "e_i1 = x - (m*f)/diff(f, x) - r\n",
    "ans = e_i1/(e_i0 ** 2)\n",
    "factor(simplify(ans))"
   ]
  }
 ],
